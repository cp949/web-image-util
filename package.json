{
  "name": "@cp949/web-image-util-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Web image utilities monorepo",
  "type": "module",
  "repository": "https://github.com/cp949/web-image-util.git",
  "author": "cp949 <cp949@naver.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "build": "turbo run build --filter=@cp949/web-image-util",
    "build:watch": "turbo run build:watch",
    "package": "turbo run package",
    "publish": "pnpm --filter @cp949/web-image-util package && cd sub/web-image-util && npm publish --access=public",
    "clean": "turbo run clean",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:node": "turbo run test:node",
    "test:ui": "turbo run test:ui",
    "test:browser": "turbo run test:browser",
    "test:coverage": "turbo run test:coverage",
    "ci": "turbo run typecheck lint format:check test:node",
    "dev": "turbo run build:watch",
    "dev:exam": "pnpm --filter @cp949/exam dev"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "@vitest/browser": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "jest-image-snapshot": "^6.5.1",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "shelljs": "^0.10.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "turbo": "^2.5.8",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vitest": "^3.2.4"
  }
}
